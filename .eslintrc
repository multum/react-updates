{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": [ "prettier"],
  "env": { "browser": true },
  "parserOptions": { "ecmaVersion": 2018, "sourceType": "module" },
  "rules": {
    "prettier/prettier": "error",
    "prefer-const": ["error", {"destructuring": "all"}],
    "lines-between-class-members": ["error", "always"]
  }
}
